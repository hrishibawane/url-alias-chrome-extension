{
	"name": "URL Alias",
	"description": "Extension to use aliases for URLs",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage", "tabs"],
	"omnibox": {
		"keyword": "hb"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/icons/logo16.png",
			"32": "/icons/logo32.png",
			"48": "/icons/logo48.png",
			"128": "/icons/logo128.png"
		}
	},
	"icons": {
		"16": "/icons/logo16.png",
		"32": "/icons/logo32.png",
		"48": "/icons/logo48.png",
		"128": "/icons/logo128.png"
	},
	"author": "Hrishikesh Bawane"
}